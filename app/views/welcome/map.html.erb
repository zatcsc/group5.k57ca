<%= render 'layouts/left_slidebar' %>

<div id="map" class="col-md-9 nopad">
  <div id="map-canvas" class="nopad"></div>
  <div id="warnings_panel" style="width:100%;height:10%;text-align:center"></div>

  <%= javascript_tag do %>
    initialize();
    <% @jam_places.each do |jam_place| %>
      jamLat.push(<%= jam_place.latitude %>);
      jamLgn.push(<%= jam_place.longtitude %>);
    <% end %>
  <% end %>

  <div id="over-map">
    <a id="toggleSidebar" class="toggles"><i class="glyphicon glyphicon-chevron-left"></i></a>
  </div>
</div>

